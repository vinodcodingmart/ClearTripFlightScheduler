<% if popular_routes['dom_dom'].present? &&  popular_routes['dom_dom'].length > 0 %>
<h2><%= t('brand.popular_dom_route_title', airline_name: @carrier_name) %></h2>
<%= render :partial=>"new_designs/bookings/shared/en/airline_amp_overview_schedule_table",:locals=>{:routes=>popular_routes['dom_dom'], :use_helper=>use_helper, :assets_path=>@assets_path, :country_code=> @country_code, :section=> 'dom',currency_code: currency_code(@country_code),domain: amp_domain,intl: intl} %>
<p>&nbsp;</p>
<% end %>
<% if popular_routes['dom_int'].present? && popular_routes['dom_int'].length > 0  %>
<h2><%= t('brand.popular_int_route_title', airline_name: @carrier_name) %></h2>
<%= render :partial=>"new_designs/bookings/shared/en/airline_amp_overview_schedule_table",:locals=>{:routes=>popular_routes['dom_int'], :use_helper=>use_helper, :assets_path=>@assets_path, :country_code=> @country_code, :section=> 'int',currency_code: currency_code(@country_code),domain: amp_domain,intl: intl} %><!--mobile-view-->
<% end %>


<!--content-desc starts-->
<div class="airline-cnt">
  <% if content["overview_content_en"].present? %>
    <%= raw content["overview_content_en"] %>
  <% else %>
      <%= render partial: "bookings/shared/en/overview_templatized_content_en",locals: {airline_name: @carrier_name,host: @host_name,top_sectors: top_sectors} %>
  <% end%>
</div>
<!--content-desc ends-->
<div class="clearfix"></div>
</div>
<div class="amp-right">
