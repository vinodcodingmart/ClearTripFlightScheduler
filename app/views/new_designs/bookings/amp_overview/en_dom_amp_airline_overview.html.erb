    </div>
    <div class="airline-brand-cnt">
      <h1><%= format_airline_name(@carrier_name, I18n.locale) %> </h1>
      <%if tag_line.present? %>
      <span><%= tag_line %></span>
      <%else%>
      <span></span>
      <%end%>
    </div>
    <div class="clearfix"></div>
    <%= render partial: "new_designs/bookings/amp_overview/overview_search_widget_en",locals: {arr_city_name:'',dep_city_name:'',dep_city_code:'',arr_city_code:'',domain: @host_name,section: @section,page_type: 'airline_overview',arr_country_code:'',dep_country_code:'',country_code: @country_code,image_present: image_present} %>
    <!-- <span class="d-btn hide-xs"><a href="<%#= @host_name%>/flights?utm_source=google&utm_medium=organic&utm_campaign=AMP_page_w_button" class="link1">Search Cheap Flights</a></span> -->
  </div>
</div>
<!--/banner-wrapper-->

<div class="clearfix"></div>
<!--/contnet Starts Here-->
<p>&nbsp;</p>
<section id="content" class="container">
