<style>
        .airport-outer-ticket {
            padding: 15px;
            background-color: #FCFCE1;
        }

        * {
            -webkit-box-sizing: border-box;
            -moz-box-sizing: border-box;
            box-sizing: border-box;
        }

        body {
            margin: 0;
            padding: 0;
            font-size: 13px;
            color: #000;
            background-color: #f9f9f9 !important;
        }

        .no-padding-ticket {
            padding: 0 !important;
        }

        .airport-details-ticket {
            box-shadow: 0 1px 2px rgba(0, 0, 0, .2);
            border: 1px solid #d9d9d9;
            background-color: #fff;
        }

        #content-ticket h2 {
            font-weight: 700;
            font-size: 20px;
            line-height: 24px;
            margin: 0;
            color: #000;
        }

        .airport-details-ticket ul {
            padding: 10px;
        }
    

        .list-unstyled-ticket {
            padding-left: 0;
            list-style: none;
        }

        .airport-details-ticket li {
            clear: both;
        }

        .airport-details-ticket label {
            width: 70px;
            color: #222;
            float: left;
        }
        
        label {
            display: inline-block;
            max-width: 100%;
            margin-bottom: 5px;
            font-weight: 700;
            font-size: 13px;
        }

        .airport-details-ticket span {
            word-wrap: break-word;
            padding: 0 0 0 5px;
            color: #444;
        }

        .airport-details-ticket h2 {
            font-size: 16px !important;
            padding: 10px 10px 10px 40px;
            background: url(https://d2w5af8s1781.cloudfront.net/waytogo-staging/images/icon_airport.png) 10px center no-repeat rgba(249, 249, 249, .32);
            border-bottom: 1px solid #d9d9d9;
        }
    </style>
<div class="airport-outer-ticket" style="font-size: 13px">
	<div class="col-sm-5 col-xs-12 no-padding-ticket">
		<div class="airport-details-ticket">
			<h2><%= dep_city_name %> <%= I18n.t('airport.airport_details')%> </h2>
			<ul class="list-unstyled-ticket ">
				<li>
					<label><%= I18n.t('airport.name')%> <span class='pull-right'>:</span></label>
					<span><%= airport_details["dep_airport_name"] %></span>
				</li>
				<li>
					<label><%= I18n.t('airport.address')%><span class='pull-right'>:</span></label>
					<span><%= airport_details["dep_airport_address"] %></span>
				</li>
				<li>
					<label><%= I18n.t('airport.phone')%> <span class='pull-right'>:</span></label>
					<span><%= airport_details["dep_airport_phone"] %></span>
				</li>
				<li>
					<label><%= I18n.t('airport.email')%> <span class='pull-right'>:</span></label>
					<span><%= airport_details["dep_airport_email"] %></span>
				</li>
				<li>
					<label><%= I18n.t('airport.website')%> <span class='pull-right'>:</span></label>
					<span><a rel="nofollow" href="<%= airport_details["dep_airport_website"] %>" target="_blank"><%= airport_details["dep_airport_website"] %></a></span>
				</li>
			</ul>
		</div>
	</div>
	<div class="col-sm-5 no-padding-ticket col-sm-offset-1 col-xs-12">
		<div class="airport-details-ticket">
			<h2><%= arr_city_name %> <%= I18n.t('airport.airport_details')%> </h2>
			<ul class="list-unstyled-ticket ">
				<li>
					<label><%= I18n.t('airport.name')%>  <span class='pull-right'>:</span></label>
					<span><%= airport_details["arr_airport_name"] %></span>
				</li>
				<li>
					<label><%= I18n.t('airport.address')%> <span class='pull-right'>:</span></label>
					<span><%= airport_details["arr_airport_address"] %></span>
				</li>
				<li>
					<label><%= I18n.t('airport.phone')%> <span class='pull-right'>:</span></label>
					<span><%= airport_details["arr_airport_phone"] %></span>
				</li>
				<li>
					<label><%= I18n.t('airport.email')%> <span class='pull-right'>:</span></label>
					<span><%= airport_details["arr_airport_email"] %></span>
				</li>
				<li>
					<label><%= I18n.t('airport.website')%> <span class='pull-right'>:</span></label>
					<span>
						<a rel="nofollow" href="<%= airport_details["arr_airport_website"] %>" target="_blank">
							<%= airport_details["arr_airport_website"] %></a>
					</span>
				</li>
			</ul>
		</div>
	</div>
	<div class="clearfix"></div>
</div>