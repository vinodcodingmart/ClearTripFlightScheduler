	      </tr></thead>
	      <tbody><tr>
	      	<% if schedule_layout_values["top_min_flights_today"].present? && schedule_layout_values["top_min_flights_today"].first["pr"].to_f > 0 %>
	      	  <td>
	            <ul class="list-unstyled">
	            	<% schedule_layout_values["top_min_flights_today"].each do|min_today| %>
									<% unless min_today["pr"].to_i == 0 %>
										<% if min_today["ad"].to_date == Date.today %>
											<% temp_date = min_today["ad"] %>
										<% elsif min_today["ad"].to_date <= Date.today %>
											<% temp_date = (Time.now).strftime('%d/%m/%Y') %>
										<% else %>
											<% temp_date = (Time.now+2.days).strftime('%d/%m/%Y') %>
										<% end %>
		              	<li>
		                  <h4>
		                    <span class="air-details">
		                    <span class="air-name-truncate"><%= min_today["aln"] %></span>
		<!--                      <span class="air-price"></span>
		 -->                </span>
											</h4> 
		                  <a href="<%= "/flights/results?from=#{schedule_layout_values["dep_airport_code"]}&to=#{schedule_layout_values["arr_airport_code"]}&depart_date=#{temp_date}&adults=1&childs=0&infants=0&page=loaded&airline=#{min_today["al"]}" %>" class="ct-btn">
													<%= "#{currency_given} #{min_today["pr"].to_i}" %>
											</a>
		                  <div class="clearfix"></div>
		                </li>
		              <% end %>
	              <% end %>
	             </ul>
	          </td>
